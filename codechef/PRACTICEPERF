#include <iostream>
#include <bits/stdc++.h>
#define ll long long
using namespace std;

int main() {
	// your code goes here
	int a[4];
	int count=4;
	for(ll i=0;i<4;i++){
	    cin>>a[i];
	}
	for(ll i=0;i<4;i++){
	    if(a[i]<10){
	        count=count - 1;
	    }
	}
	cout<<count<<endl;
	return 0;
}
